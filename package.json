{
	"name": "react-redux-es6",
	"version": "1.0.0",
	"description": "getting started template for react developers",
	"scripts": {
		"start:server": "webpack-dev-server --config ./webpack.config.dev.js --hot --inline --progress --open",
		"start": "npm-run-all --parallel start:server lint:watch test:watch",
		"lint": "esw webpack.config.* src --color",
		"lint:watch": "yarn run lint --watch",
		"test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
		"test:watch": "yarn run test --watch",
		"clean-dist": "rimraf ./dist && mkdir dist",
		"prebuild": "npm-run-all clean-dist test lint",
		"build": "webpack -p --config ./webpack.config.prod.js --progress",
		"postbuild": "babel-node buildScripts/distServer.js"
	},
	"author": "Israel Orbach",
	"license": "MIT",
	"dependencies": {
		"axios": "^0.17.1",
		"copy-to-clipboard": "^3.0.8",
		"file-loader": "^1.1.6",
		"leaflet": "^1.3.1",
		"leaflet-contextmenu": "^1.4.0",
		"leaflet-draw": "^0.4.13",
		"leaflet-mouse-position": "^1.0.1",
		"material-ui": "^0.20.0",
		"prop-types": "^15.6.0",
		"react": "^16.2.0",
		"react-dom": "^16.2.0",
		"react-redux": "^5.0.6",
		"redux": "^3.7.2",
		"redux-thunk": "^2.2.0",
		"sweetalert": "^2.1.0"
	},
	"devDependencies": {
		"babel-cli": "6.26.0",
		"babel-core": "6.26.0",
		"babel-eslint": "^8.2.1",
		"babel-loader": "7.1.2",
		"babel-plugin-syntax-dynamic-import": "^6.18.0",
		"babel-plugin-transform-async-to-generator": "^6.24.1",
		"babel-polyfill": "^6.26.0",
		"babel-preset-env": "^1.6.1",
		"babel-preset-latest": "6.24.1",
		"babel-preset-react": "^6.24.1",
		"babel-register": "6.26.0",
		"chai": "4.1.2",
		"compression": "1.7.1",
		"cross-env": "5.1.3",
		"css-loader": "^0.28.9",
		"enzyme": "^3.3.0",
		"enzyme-adapter-react-16": "^1.1.1",
		"eslint": "4.14.0",
